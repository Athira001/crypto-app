{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "max-len": ["error", 100, 2, {
      "ignoreUrls": true,
      "ignoreComments": true,
      "ignoreRegExpLiterals": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true
    }]
  }
}
